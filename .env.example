# Planbok API
PLANBOK_API_URL=https://api.planbok.io/v2
PLANBOK_API_KEY=your-api-key-here

# Organization Credentials (get from your account setup)
ORGANIZATION_SECRET=your-organization-secret-hex
ORGANIZATION_PK="-----BEGIN PUBLIC KEY-----
...your-public-key...
-----END PUBLIC KEY-----"

# Shared Wallet Set (optional - will be created on first use if not set)
# Get this after first run: the console will output the created wallet set ID
WALLET_SET_ID=

# Backend
PORT=4000
SESSION_SECRET=your-session-secret-here

# Frontend (configured in frontend/.env.local)
NEXT_PUBLIC_API_URL=http://localhost:4000/api
